{
    "name": "Simple TabFloater",
    "description": "将标签页转换为智能悬浮窗",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "tabs",
        "nativeMessaging",
        "storage",
        "scripting"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "js/main.js",
        "type": "module"
    },
    "action": {
        "default_title": "Float tab!",
        "default_icon": {
            "16": "images/icons/16.png",
            "32": "images/icons/32.png",
            "48": "images/icons/48.png",
            "128": "images/icons/128.png"
        }
    },
    "icons": {
        "16": "images/icons/16.png",
        "32": "images/icons/32.png",
        "48": "images/icons/48.png",
        "128": "images/icons/128.png"
    },
    "commands": {
        "float-tab": {
            "suggested_key": {
                "default": "Alt+K",
                "mac": "Alt+K"
            },
            "description": "将标签页转为悬浮窗",
            "global": true
        },
        "unfloat-tab": {
            "suggested_key": {
                "default": "Alt+L",
                "mac": "Alt+L"
            },
            "description": "将悬浮窗还原为标签页",
            "global": true
        }
    },
    "key": ""
} 
